@{
    ViewBag.Title = "GrtWastePercent";
    Layout = "~/Views/Shared/_Layout_List.cshtml";
}

<script src="~/Scripts/JQueryTool/CheckVal.js"></script>
<script src="~/Scripts/JQueryTool/Method.js"></script>
<script src="~/Scripts/Machine/GrtWastePercent.js"></script>

<div id="searchGRTToolBar">
    日期:<input id="searchGRTDate" type="text" style="" class="easyui-datebox" required="required" data-options="prompt:'日期-(yyyymmdd)'" />
    线别:<input id="searchGRTInputLine" class="easyui-combobox" data-options="
                       textField: 'text',
                       valueField: 'value',
		                data: [{
			                text: '所有线别',
			                value: '0'
		                },{
			                text: '01',
			                value: '1'
		                },{
			                text: '02',
			                value: '2'
		                },{
			                text: '03',
			                value: '3'
		                },{
			                text: '04',
			                value: '4'
		                },{
			                text: '05',
			                value: '5'
		                },{
			                text: '06',
			                value: '6'
		                },{
			                text: '07',
			                value: '7'
		                }]" />
    班别:<input id="searchGRTInputShift" class="easyui-combobox" data-options="
              textField: 'text' ,
              valueField: 'value' ,
              data: [{
              text: 'A' ,
              value: 'A'
              },{
              text: 'B' ,
              value: 'B'
              }]" />
    <a id="btnGRTSearch" class="easyui-linkbutton" href="#" data-options="iconCls:'icon-search'">查询</a>
</div>
<table id="GRTTable" class="easyui-datagrid" style="height:543px"></table>
